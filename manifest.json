{
  "name": "Page density",
  "version": "0.0.1",
  "description": "Calculates the page density of a set of HTML elements",
  "icons": {
    "16": "icons/page-density-16.png",
    "48": "icons/page-density-48.png",
    "128": "icons/page-density-128.png"
  },
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_title": "Page density",
    "default_popup": "browserAction/popup.html"
  },
  "options_page": "options/options.html",
  "manifest_version": 2
}
